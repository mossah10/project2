<apex:page id="SymptomPage" controller="GetAllSymptoms" action="{!onLoad}">
    <apex:form id="frm">
        <apex:pageBlock title="Welcome to E-Doc">
            <table>
                <apex:pageBlock title="Please check off your symptoms:">
                    <apex:repeat value="{!wrapClassList}" var="jsonTable" id="JSONTable">
                        <tr>
                            <td> <apex:outputText value="{!jsonTable.name}"/> </td>
                            <td> <apex:inputCheckbox value="{!jsonTable.flag}"/> </td>
                        </tr>
                    </apex:repeat>
                </apex:pageBlock>
            </table>
        </apex:pageBlock>
    </apex:form>
</apex:page>